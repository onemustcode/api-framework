{
    "name": "onemustcode/api",
    "description": "A simple to use api framework.",
    "keywords": ["api", "framework"],
    "type": "project",
    "licence": "MIT",
    "authors": [
        {
            "name": "Jeffrey Buitenhuis",
            "email": "jeffrey@onemustcode.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "php-di/php-di": "^6.0",
        "symfony/http-foundation": "^4.2",
        "symfony/routing": "^4.2",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "OneMustCode\\ApiFramework\\": "src/"
        }
    }
}
